@using hack24.web.Resources
@model ProfileResource
@{
	ViewBag.Title = "Profile";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>View</h2>

<p>@Model.FirstName</p>
<p>@Model.LastName</p>
<p>@Model.Bio</p>
<p>@Model.JobTitle</p>
<p>@Model.ProfileImage</p>
<h3>Tags</h3>
<ul>
	@foreach (var tag in Model.Tags)
	{
		<li>@tag.DisplayName</li>
	}
</ul>