@model hack24.web.Resources.TagPopularityModel

@{
	ViewBag.Title = "Competency Nebula";
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 style="text-align: center; margin: 30px 0;">Competency Nebula</h2>
<div class="tag-data" >
	@foreach (var tagdata in @Model.TagData)
	{
		<a style="color: #2aa696;" href="@Url.Action("ByTag", "Profile", new { tagdata.Key.Name})">@tagdata.Key.DisplayName (@tagdata.Value)</a><span style="color: #000;">&nbsp;&nbsp;|&nbsp;&nbsp;</span>
	}
</div>